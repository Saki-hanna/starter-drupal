{#
/**
 * @file document_entity.html.twig
 * Default theme implementation to present Document entity data.
 *
 * This template is used when viewing Document entity pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_document_entity()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('document_entity') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
